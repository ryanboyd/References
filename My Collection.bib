@article{amith_representing_2018,
	title = {Representing vaccine misinformation using ontologies},
	volume = {9},
	issn = {2041-1480},
	url = {https://doi.org/10.1186/s13326-018-0190-0},
	doi = {10.1186/s13326-018-0190-0},
	abstract = {In this paper, we discuss the design and development of a formal ontology to describe misinformation about vaccines. Vaccine misinformation is one of the drivers leading to vaccine hesitancy in patients. While there are various levels of vaccine hesitancy to combat and specific interventions to address those levels, it is important to have tools that help researchers understand this problem. With an ontology, not only can we collect and analyze varied misunderstandings about vaccines, but we can also develop tools that can provide informatics solutions.},
