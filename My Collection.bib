	month = sep,
	year = {1999},
	keywords = {Motivation, Attribution, college students, Coping Behavior, Goals, Mastery Learning, mediation of mastery-oriented coping by effort vs, Self-Efficacy, test of incremental vs entity implicit self-theori, Theories},
	pages = {588--599},
