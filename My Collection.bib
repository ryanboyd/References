	journal = {Journal of Clinical Epidemiology},
	author = {IntHout, Joanna and Ioannidis, John P. A. and Borm, George F. and Goeman, Jelle J.},
	month = aug,
	year = {2015},
	keywords = {Meta-analysis, Between-study heterogeneity, Cochrane Database of systematic reviews (CDSR), Random-effects model, Randomized controlled trial, Trial size},
	pages = {860--869},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Ryan\\Zotero\\storage\\3RS3ZMMJ\\IntHout et al. - 2015 - Small studies are more heterogeneous than large on.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Ryan\\Zotero\\storage\\I7LDIEME\\S089543561500164X.html:text/html}
