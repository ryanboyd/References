	pages = {459--471},
	file = {Full Text PDF:C\:\\Users\\Ryan\\Zotero\\storage\\Y6ZUUVZ8\\Vilchinsky and Kravetz - 2005 - How Are Religious Belief and Behavior Good for You.pdf:application/pdf;Snapshot:C\:\\Users\\Ryan\\Zotero\\storage\\Z6BZTYJ7\\j.1468-5906.2005.00297.html:text/html}
}

@article{steger_understanding_2008,
	title = {Understanding the {Search} for {Meaning} in {Life}: {Personality}, {Cognitive} {Style}, and the {Dynamic} {Between} {Seeking} and {Experiencing} {Meaning}},
