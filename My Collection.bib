@article{conner_experience_2009,
	title = {Experience {Sampling} {Methods}: {A} {Modern} {Idiographic} {Approach} to {Personality} {Research}},
	volume = {3},
	issn = {1751-9004},
	shorttitle = {Experience {Sampling} {Methods}},
