@article{psathas_general_1969,
	title = {The general inquirer: {Useful} or not?},
	volume = {3},
	issn = {1572-8412},
	url = {http://dx.doi.org/10.1007/BF02401609},
	doi = {10.1007/BF02401609},
	abstract = {I share the reviewer's estimate that the book was unduly optimistic in proposing that the system could be applied fairly easily and quickly to a variety of problems. It is my considered opinion that although the systemcan be applied in a variety of substantive areas, it can be applied neither quickly nor easily. Unless the investigator is willing to make a long-term commitment to research with computers, the use of the General Inquirer system is not to be recommended at this time. Perhaps in another five to ten years, when systems have been devised that are more flexible, much better documented, and user oriented, the kinds of hopes and promises which the book stimulated will become a reality. For those who wish to focus their research on computer program development for natural language processing, the General Inquirer already provides a reality; for those who wish substantive results by applying a “ready-made” system,caveat emptor.},
