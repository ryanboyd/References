	pages = {2015--2020},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Ryan\\Zotero\\storage\\3ABFD36U\\7732347.html:text/html}
}

@article{shin_political_2017,
	title = {Political rumoring on {Twitter} during the 2012 {US} presidential election: {Rumor} diffusion and correction},
