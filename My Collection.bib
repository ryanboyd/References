	year = {2012},
	pmid = {21861772},
	keywords = {depression, emotional approach coping, negative affect, positive affect, repetitive thought},
	pages = {3--21},
	file = {Accepted Version:C\:\\Users\\Ryan\\Zotero\\storage\\XPHGTKSD\\Segerstrom et al. - 2012 - Episodic repetitive thought dimensions, correlate.pdf:application/pdf;Snapshot:C\:\\Users\\Ryan\\Zotero\\storage\\MUY24E6B\\10615806.2011.html:text/html}
}

