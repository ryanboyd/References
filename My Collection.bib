	keywords = {classification, alcoholism, comorbidity, dsm, externalizing disorders, iii-r, internalizing disorders, lifetime, mental disorders, national-comorbidity-survey, personality traits, prevalence, psychiatric-disorders, psychopathology, sample, traits},
	pages = {1245--1259}
