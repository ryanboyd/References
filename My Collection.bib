	pmid = {17201789},
	keywords = {Ego, Emotions, Empathy, Female, Humans, Male, Adult, Students, Visual Perception, Facial Expression, Communication, Intention, Orientation, Power (Psychology)},
	pages = {1068--1074}
}

@article{edens_interpersonal_2009,
