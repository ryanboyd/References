@article{smith_interpersonal_2010,
	title = {Interpersonal circumplex descriptions of psychosocial risk factors for physical illness: application to hostility, neuroticism, and marital adjustment},
	volume = {78},
	issn = {1467-6494},
	shorttitle = {Interpersonal circumplex descriptions of psychosocial risk factors for physical illness},
	doi = {10.1111/j.1467-6494.2010.00641.x},
	abstract = {Personality risk factors for physical illness are typically studied individually and apart from risk factors reflecting the social environment, potentially fostering a piecemeal understanding of psychosocial influences on health. Because it can be used to describe both personality and social relationship processes, the interpersonal circumplex (IPC) provides an integrative approach to psychosocial risk. In 301 married couples we examined IPC correlates of 3 risk factor domains: anger, hostility, and aggressiveness; neuroticism; and marital adjustment. Risk factors displayed IPC locations ranging from hostile dominance (e.g., verbal aggressiveness, marital conflict) to hostility (e.g., anger) to hostile submissiveness (e.g., anxiety, depression); protective factors (marital satisfaction and support) reflected warmth or friendliness in the IPC. Similar descriptions were found using self-reports and spouse ratings of IPC dimensions, indicating that interpersonal styles associated with risk factors do not simply reflect common method variance. Findings identify interpersonal processes reflecting low affiliation or high hostility as a common component of risk and indicate distinctions among risk factors along the dominance dimension.},
	language = {eng},
	number = {3},
	journal = {Journal of Personality},
	author = {Smith, Timothy W. and Traupman, Emily K. and Uchino, Bert N. and Berg, Cynthia A.},
	month = jun,
	year = {2010},
	pmid = {20573134},
	pmcid = {PMC5045262},
	keywords = {Personality, Social Adjustment, Female, Humans, Male, Adult, Middle Aged, Personality Inventory, Interpersonal Relations, Surveys and Questionnaires, Spouses, Regression Analysis, Hostility, Aged, Marriage, Risk Factors, Social Environment},
	pages = {1011--1036}
}

@article{smith_hostility_2004,
